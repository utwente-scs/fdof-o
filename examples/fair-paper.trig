@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:default {
  :paper rdf:type fdof:InformationObject ;
          fdof:hasInformationObjectType fdoft:Article ;
          fdof:identifier "https://www.nature.com/articles/sdata201618" ;
          fdof:identifier "https://doi.org/10.1038/sdata.2016.18" ;
          fdof:isMaterializedBy :paperPdf, :paperHtml ;
          fdof:hasMetadata :natureMetadata, :fdofMetadata .

  :paperPdf rdf:type fdof:FAIRMediaObject ;
          fdof:gupri "https://www.nature.com/articles/sdata201618.pdf" ;
          fdof:hasEncodingFormat <https://www.iana.org/assignments/media-types/application/pdf> .

  :paperHtml rdf:type fdof:MediaObject ;
          fdof:identifier "https://doi.org/10.1038/sdata.2016.18", "https://www.nature.com/articles/sdata201618" ;
          fdof:hasEncodingFormat <https://www.iana.org/assignments/media-types/text/html> .

  :exampleTtl rdf:type fdof:FAIRMediaObject ;
          fdof:identifier "https://w3id.org/fdof/examples/fair-paper#exampleTtl" ;
          fdof:hasEncodingFormat <https://www.iana.org/assignments/media-types/text/turtle> .

  :natureMetadata rdf:type fdof:MetadataRecord ;
              fdof:identifier "https://www.nature.com/articles/sdata201618" ;
              fdof:identifier "https://doi.org/10.1038/sdata.2016.18" ;
              fdof:isMaterializedBy :materializationHtml ;
              fdof:isMetadataOf :paper .

  :fdofMetadata rdf:type fdof:FAIRMetadataRecord ;
              fdof:hasInformationObjectType fdoft:ArticleMetadata ;
              fdof:gupri "https://w3id.org/fdof/examples/fair-paper#fdofMetadata" ;
              fdof:isMaterializedBy :exampleTtl ;
              fdof:isMetadataOf :paper .

  :pdfMetadata rdf:type fdof:FAIRMetadataRecord ;
              fdof:hasInformationObjectType fdoft:PdfMetadata ;
              fdof:gupri "https://w3id.org/fdof/examples/fair-paper#pdfMetadata" ;
              fdof:isMaterializedBy :exampleTtl ;
              fdof:isMetadataOf :paperPdf .

  :htmlMetadata rdf:type fdof:FAIRMetadataRecord ;
              fdof:hasInformationObjectType fdoft:HtmlMetadata ;
              fdof:gupri "https://w3id.org/fdof/examples/fair-paper#htmlMetadata" ;
              fdof:isMaterializedBy :exampleTtl ;
              fdof:isMetadataOf :paperPdf .

  :ttlMetadata rdf:type fdof:FAIRMetadataRecord ;
              fdof:hasInformationObjectType fdoft:TtlMetadata ;
              fdof:gupri "https://w3id.org/fdof/examples/fair-paper#ttlMetadata" ;
              fdof:isMaterializedBy :exampleTtl ;
              fdof:isMetadataOf :paperPdf .

}

